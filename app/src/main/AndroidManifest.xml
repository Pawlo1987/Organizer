<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.organizer">
    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_main_player"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_main_player"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AuthorizationActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".UnauthorizedPartActivity" />
        <activity android:name=".activity.ShowAllEventActivity" />
        <activity android:name=".CreateEventActivity" />
        <activity android:name=".CreateAccountActivity" />
        <activity android:name=".CreateCityActivity" />
        <activity android:name=".CreateFieldActivity" />
        <activity android:name=".SelectParticipantsActivity" />
        <activity android:name=".activity.ShowAuthUserEventsActivity" />
        <activity android:name=".ShowAuthUserEventsRecyclerAdapter" />
        <activity android:name=".activity.AdvertisingAndInformationActivity" />
        <activity android:name=".AdvertisingAndInformationRecyclerAdapter" />
        <activity android:name=".CreateNewsNoteActivity" />
        <activity
            android:name=".NavigationDrawerLogInActivity"
            android:label="@string/title_activity_navigation_drawer_log_in"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".EditEventActivity"></activity>
    </application>

</manifest>